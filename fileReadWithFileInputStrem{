public fileReadWithFileInputStrem{
	
	public static void main(String[] args){
		File file= new File("MyFile.txt");

		if(!file.exists()){
			file.createFile();
		}

		try{
			int i;
			FileInputStream fis=new FileInputStream();
			while((i=is.read())!=-1)
			{	
			fis.read((char)i);
			fis.close();
			}
		}catch(IOExceptions e){
			e.printstackTrace();
		}

	}
}
